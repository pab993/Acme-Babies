
package forms;

import domain.DomainEntity;

public class AssessmentForm extends DomainEntity {

	// Constructors
	// ====================================================================================

	// Attributes
	// ====================================================================================

	private String	title;
	private String	text;
	private int		rate;
	private int		idComentable;


	public String getTitle() {
		return this.title;
	}
	public void setTitle(final String title) {
		this.title = title;
	}

	public String getText() {
		return this.text;
	}
	public void setText(final String text) {
		this.text = text;
	}
	public int getRate() {
		return this.rate;
	}
	public void setRate(final int rate) {
		this.rate = rate;
	}
	public int getIdComentable() {
		return this.idComentable;
	}
	public void setIdComentable(final int idComentable) {
		this.idComentable = idComentable;
	}

}
