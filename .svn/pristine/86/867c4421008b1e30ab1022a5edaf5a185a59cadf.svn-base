<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd		
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd		
	">

	<util:list id="converters">
		<!-- 
			Add your own converters using entries of the following form: 
			<bean class="converters.Class1ToClass2" /> 
		-->
		
		<bean class="converters.ComentableToStringConverter"/>
		<bean class="converters.StringToComentableConverter"/>
		
		<bean class="converters.AssessmentToStringConverter"/>
		<bean class="converters.StringToAssessmentConverter"/>
		
		<bean class="converters.InscriptionToStringConverter"/>
		<bean class="converters.StringToInscriptionConverter"/>		
		
		<bean class="converters.KindergartenToStringConverter"/>
		<bean class="converters.StringToKindergartenConverter"/>
		
		<bean class="converters.CaretakerToStringConverter"/>
		<bean class="converters.StringToCaretakerConverter"/>
		
		<bean class="converters.LessonToStringConverter"/>
		<bean class="converters.StringToLessonConverter"/>
		
		<bean class="converters.ActorToStringConverter"/>
		<bean class="converters.StringToActorConverter"/>
		
		<bean class="converters.CustomerToStringConverter"/>
		<bean class="converters.StringToCustomerConverter"/>
		
		<bean class="converters.InvoiceToStringConverter"/>
		<bean class="converters.StringToInvoiceConverter"/>
		
		<bean class="converters.ApplyToStringConverter"/>
		<bean class="converters.StringToApplyConverter"/>
		
		<bean class="converters.OfferToStringConverter"/>
		<bean class="converters.StringToOfferConverter"/>

	</util:list>
	
</beans>